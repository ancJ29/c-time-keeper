{
  "files.exclude": {
    "**/node_modules": true
  },
  "cSpell.ignorePaths": [
    "./src/configs/i18n/vi.ts"
  ],
  "cSpell.words": [
    "daygrid",
    "fullcalendar",
    "mantine",
    "tabler",
    "timegrid"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.addMissingImports": "explicit",
    "source.removeUnusedImports": "explicit"
  },
}
